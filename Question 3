/*Která kategorie potravin zdražuje nejpomaleji (je u ní nejnižší percentuální meziroční nárůst)?

*/

SELECT
	ls.name, 
	YEAR(ls.date_to) AS `YEAR`,
	YEAR(ls2.date_to) AS `year_prev`,
	round ( (ls.value - ls2.value) / ls2.value,2) AS price_growth_percent
FROM
	t_Lenka_Sedlackova_project_SQL_primary_final AS ls
JOIN 
	t_Lenka_S edlackova_project_SQL_primary_final AS ls2
ON
	ls.name = ls2.name
	AND YEAR(ls.date_to) = YEAR(ls2.date_to)+1
	AND  YEAR(ls.date_to) < 2019
;
